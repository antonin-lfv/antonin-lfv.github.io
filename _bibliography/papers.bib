@article{autoencoderETecggeneration,
  title={Auto Encodeur variationnel pour la génération d’électrocardiogrammes},
  author = {Antonin L.},
  abstract = {Cet article présente une application Streamlit innovante capable de générer des électrocardiogrammes (ECG) synthétiques en utilisant un autoencodeur variationnel. L'outil offre une nouvelle manière d'augmenter les ensembles de données ECG pour la recherche médicale et le développement d'algorithmes de diagnostic. En combinant les avantages de l'interface utilisateur intuitive de Streamlit avec la puissance des modèles génératifs, cette application sert de pont entre les domaines de la médecine et de l'apprentissage automatique.},
  year = {2023},
  month = {April},
