@article{autoencoderETanomalydetection,
  title={Auto Encodeur de débruitage pour la détection d'anomalies vibratoires},
  author={Antonin L.},
  abstract={Ce projet permet de détecter les chocs en temps réel à l'aide d'un modèle d'autoencodeur de débruitage et d'un capteur d'accélération. L'objectif est d'identifier les chocs parmi les données du capteur en éliminant le bruit et les variations normales d'accélération. Nous utiliserons le capteur MPU-6050 (GY-521) et un Arduino Uno pour collecter les données d'accélération. L'interface pour visualiser les données en temps réel est développé avec Streamlit.},
